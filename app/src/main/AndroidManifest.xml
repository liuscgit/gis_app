<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.survey.schd.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TableActivity"
            android:label="TableActivity">
        </activity>

        <activity android:name=".TableGeologySurveyActivity"
            android:label="TableGeologySurveyActivity">
        </activity>

        <activity android:name=".TableGeoSurveyActivity"
            android:label="TableGeologySurveyActivity">
        </activity>

        <activity android:name=".TableSlopeSurveyActivity"
            android:label="TableSlopeSurveyActivity">
        </activity>

        <activity android:name=".TableBadGeology"
            android:label="TableBadGeology">
        </activity>

        <activity android:name=".LineSymbolSelectActivity"
            android:label="LineSymbolSelectActivity">
        </activity>

        <activity android:name=".PointSymbolSelectActivity"
            android:label="PointSymbolSelectActivity">
        </activity>

        <activity android:name=".PolygonSymbolSelectActivity"
            android:label="PolygonSymbolSelectActivity">
        </activity>

        <activity android:name=".pile_activity.PileSelector"
            android:label="PileSelector">
        </activity>

    </application>

</manifest>