<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF"
    android:focusable="true"
    android:focusableInTouchMode="true" >

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:fadingEdge="none">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="10.0dip"
            android:paddingTop="10.0dip"
            android:paddingRight="10.0dip"
            android:paddingBottom="60.0dip">


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">


                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="1.0dip"
                    android:shrinkColumns="0"
                    android:stretchColumns="0">

                    <!--<View-->
                    <!--android:layout_width="fill_parent"-->
                    <!--android:layout_height="1.0px"-->
                    <!--/>-->

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="1.0dip"
                        android:shrinkColumns="0"
                        android:stretchColumns="0">

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="10.0dip"
                            android:layout_marginBottom="1.0dip"
                            android:layout_toRightOf="@+id/example"
                            android:gravity="center"
                            android:layout_weight="2"
                            android:paddingLeft="5.0dip"
                            android:text="地质调查记录表(按路段小节归纳用)"
                            android:textColor="#ff888888"
                            android:textSize="18.0sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingTop="1.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dip"
                            android:layout_weight="2"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:paddingLeft="17.0dip"
                            android:text="一般路段"
                            android:textColor="#ff333333"
                            android:background="#FFFFFF"
                            android:textSize="16.0sp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:textColor="#ff888888"
                            android:background="#FFFFFF" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dip"
                            android:layout_weight="2"
                            android:gravity="center_vertical"
                            android:layout_marginRight="1.0dip"
                            android:paddingLeft="17.0dip"
                            android:text="地点:"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/qihouEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:gravity="center_vertical"
                            android:textColor="#ff888888"
                            android:background="#FFFFFF"
                            android:textSize="16sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingTop="1.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:background="@drawable/shape_et_bottom_line"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dip"
                            android:layout_weight="1"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:paddingLeft="17.0dip"
                            android:text="起讫桩号"
                            android:textColor="#ff333333"
                            android:background="#FFFFFF"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/qiqizhuanghaoEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:textColor="#ff888888"
                            android:background="#FFFFFF"
                            android:textSize="16.0sp" />

                    </TableRow>

                    <TableRow
                        android:id="@+id/freezingDeepRow"
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:background="@drawable/shape_et_bottom_line"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="240dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:paddingLeft="10.0dip"
                            android:background="#FFFFFF"
                            android:layout_marginRight="1.0dip"
                            android:text="路基填、挖
形式或构造物类型
"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/shiyanquyangEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:background="#FFFFFF"
                            android:gravity="center_vertical"
                            android:paddingRight="20.0dip"
                            android:textColor="#ff888888"
                            android:textSize="16.0sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:background="@drawable/shape_et_bottom_line"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="480dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:paddingLeft="10.0dip"
                            android:scrollbars="vertical"
                            android:singleLine="false"
                            android:layout_marginRight="1.0dip"
                            android:background="#FFFFFF"
                            android:text="调绘与简易勘探查明的基本工程地质及水文地质  条 件"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/chunigongchengEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:background="#FFFFFF"
                            android:gravity="center_vertical"
                            android:paddingRight="20.0dip"
                            android:textColor="#ff888888"
                            android:textSize="16.0sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:background="@drawable/shape_et_bottom_line"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="160dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:paddingLeft="10.0dip"
                            android:scrollbars="vertical"
                            android:singleLine="false"
                            android:layout_marginRight="1.0dip"
                            android:background="#FFFFFF"
                            android:text="初拟工程对策措施"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/shuiwendizhiEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:background="#FFFFFF"
                            android:gravity="center_vertical"
                            android:paddingRight="20.0dip"
                            android:textColor="#ff888888"
                            android:textSize="16.0sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="392dp"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:background="@drawable/shape_et_bottom_line"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="160dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:paddingLeft="17.0dip"
                            android:scrollbars="vertical"
                            android:singleLine="false"
                            android:layout_marginRight="1.0dip"
                            android:background="#FFFFFF"
                            android:text="须进一步落实的地质问题与初拟勘察申请的工作量布置"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/beizhuEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:background="#FFFFFF"
                            android:gravity="center_vertical"
                            android:paddingRight="20.0dip"
                            android:textColor="#ff888888"
                            android:textSize="16.0sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_marginLeft="2.0dip"
                        android:layout_marginRight="2.0dip"
                        android:paddingTop="1.0dip"
                        android:paddingLeft="1.0dip"
                        android:paddingRight="1.0dip"
                        android:paddingBottom="1.0dip">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dip"
                            android:layout_weight="2"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:paddingLeft="17.0dip"
                            android:text="记录:"
                            android:textColor="#ff333333"
                            android:background="#FFFFFF"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/jiluEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:layout_marginRight="1.0dip"
                            android:gravity="center_vertical"
                            android:textColor="#ff888888"
                            android:background="#FFFFFF"
                            android:textSize="16.0sp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dip"
                            android:layout_weight="2"
                            android:gravity="center_vertical"
                            android:layout_marginRight="1.0dip"
                            android:paddingLeft="17.0dip"
                            android:text="复核:"
                            android:textColor="#ff333333"
                            android:textSize="16.0sp" />

                        <EditText
                            android:id="@+id/fuheEditText"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"
                            android:gravity="center_vertical"
                            android:textColor="#ff888888"
                            android:background="#FFFFFF"
                            android:textSize="16sp" />

                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <Button
                android:id="@+id/geo_surebutton"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="保存" />
        </LinearLayout>

    </ScrollView>

</RelativeLayout>



